XTP_IDR_CUSTOMIZEMENU MENU DISCARDABLE 
BEGIN
    POPUP "Customizar"
    BEGIN
        MENUITEM "Reiniciar",                      XTP_ID_CUSTOMIZE_RESET
        MENUITEM "Excluir",                     XTP_ID_CUSTOMIZE_DELETE
        MENUITEM SEPARATOR
        MENUITEM "Nome:",                      XTP_ID_CUSTOMIZE_NAME
        MENUITEM SEPARATOR
        MENUITEM "Copiar imagem do botão",          XTP_ID_CUSTOMIZE_COPYBUTTONIMAGE

        MENUITEM "Colar imagem do botão",         XTP_ID_CUSTOMIZE_PASTEBUTTONIMAGE

        MENUITEM "Imagem do botão Redefinir",         XTP_ID_CUSTOMIZE_RESETBUTTONIMAGE

        MENUITEM "Editar imagem do botão...",       XTP_ID_CUSTOMIZE_EDITBUTTONIMAGE

        MENUITEM "Mudar imagem do botão",        XTP_ID_CUSTOMIZE_CHANGEBUTTONIMAGE

        MENUITEM SEPARATOR
        MENUITEM "Estilo Padrão",              XTP_ID_CUSTOMIZE_DEFAULTSTYLE

        MENUITEM "Somente texto",                  XTP_ID_CUSTOMIZE_TEXTONLY
        MENUITEM "Imagem e Texto",             XTP_ID_CUSTOMIZE_IMAGEANDTEXT

        MENUITEM SEPARATOR
        MENUITEM "Iniciar um grupo",              XTP_ID_CUSTOMIZE_BEGINAGROUP
    END
END

