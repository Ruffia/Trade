<Grid xmlns='http://schemas.microsoft.com/winfx/2006/xaml/presentation' xmlns:x='http://schemas.microsoft.com/winfx/2006/xaml'>
	<Border x:Name='Border' Background='#FFFFFFFF' />
	<Canvas x:Name='Canvas' HorizontalAlignment='Center' VerticalAlignment='Center'>
		<Rectangle Canvas.Left='2' x:Name='Stroke' Width='8' Height='8' StrokeThickness='1' Stroke='#FF000000' />
		<Rectangle Canvas.Top='2'  x:Name='Stroke' Width='8' Height='8' StrokeThickness='1' Stroke='#FF000000' />
		<Border Canvas.Left='1' Canvas.Top='3' Padding='3' Background='{x:Static Theme.Colors.Frame.ButtonRestorePressedBrush}' />
	</Canvas>
</Grid>
