    PUSHBUTTON      "Zoome ut",AFX_ID_PREVIEW_ZOOMOUT,342,2,61,12,BS_FLAT
    PUSHBUTTON      "Lukk",AFX_ID_PREVIEW_CLOSE,410,2,61,12,BS_FLAT
END


