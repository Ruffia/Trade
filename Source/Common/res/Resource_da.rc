XTP_IDR_MENU_HEADERFOOTER_FORMATS MENU DISCARDABLE 
BEGIN
    POPUP "HF"
    BEGIN
        MENUITEM "&&d   Kort datoformat",    XTP_ID_HF_FORMAT_D1
        MENUITEM "&&D   Langt datoformat",     XTP_ID_HF_FORMAT_D2
        MENUITEM SEPARATOR
        MENUITEM "&&t  Tid",                 XTP_ID_HF_FORMAT_T1
        MENUITEM "&&T  Tid (24 timer)",       XTP_ID_HF_FORMAT_T2 
        MENUITEM SEPARATOR
        MENUITEM "&&p   Sidenummer",          XTP_ID_HF_FORMAT_P1
        MENUITEM "&&P   Antal sider",      XTP_ID_HF_FORMAT_P2
        MENUITEM SEPARATOR
        MENUITEM "&&b  Venstrejustering &&b Centerjustering &&b Højre justering", XTP_ID_HF_FORMAT_B
        MENUITEM SEPARATOR
        MENUITEM "&&w   Vinduets titel",         XTP_ID_HF_FORMAT_W
        MENUITEM "&&&&  Ampersand (&&)",       XTP_ID_HF_FORMAT_UMP
        MENUITEM "\\n   Ny linje",             XTP_ID_HF_FORMAT_N
    END
END

