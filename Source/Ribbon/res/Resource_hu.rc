XTP_IDR_RIBBONCUSTOMIZEMENU MENU DISCARDABLE 
BEGIN
    POPUP "Gyors hozzáférés"
    BEGIN
        MENUITEM "Távolítsa el a Gyorselérési eszköztárról",               XTP_ID_RIBBONCUSTOMIZE_REMOVE
        MENUITEM "Hozzáadás a Gyorselérési eszköztárhoz",                    XTP_ID_RIBBONCUSTOMIZE_ADD
        MENUITEM SEPARATOR
        MENUITEM "A Gyorselérési eszköztár testreszabása...",              XTP_ID_RIBBONCUSTOMIZE
        MENUITEM "Gyorselérési eszköztár megjelenítése a szalag alatt",     XTP_ID_RIBBONCUSTOMIZE_QUICKACCESSBELOW
