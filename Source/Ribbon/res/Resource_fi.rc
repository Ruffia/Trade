XTP_IDR_RIBBONCUSTOMIZEMENU MENU DISCARDABLE 
BEGIN
    POPUP "Nopea pääsy"
    BEGIN
        MENUITEM "Poista pikatyökalupalkista",               XTP_ID_RIBBONCUSTOMIZE_REMOVE
        MENUITEM "Lisää pikatyökalupalkkiin",                    XTP_ID_RIBBONCUSTOMIZE_ADD
        MENUITEM SEPARATOR
        MENUITEM "Mukauta pikatyökalupalkkia...",              XTP_ID_RIBBONCUSTOMIZE
        MENUITEM "Näytä pikatyökalupalkki nauhan alla",     XTP_ID_RIBBONCUSTOMIZE_QUICKACCESSBELOW
        MENUITEM "Näytä pikatyökalupalkki nauhan yläpuolella",     XTP_ID_RIBBONCUSTOMIZE_QUICKACCESSABOVE
        MENUITEM SEPARATOR
        MENUITEM "Minimoi nauha",              XTP_ID_RIBBONCUSTOMIZE_MINIMIZE
    END
END

XTP_IDR_RIBBONCUSTOMIZEMENUMORE MENU DISCARDABLE 
BEGIN
    POPUP "Nopea pääsy"
    BEGIN
        MENUITEM "Mukauta pikatyökalupalkkia",                 XTP_ID_RIBBONCUSTOMIZE_LABEL
        MENUITEM "[Komennot]",                                      XTP_ID_RIBBONCUSTOMIZE_COMMANDS
        MENUITEM SEPARATOR
        MENUITEM "Lisää komentoja...",                               XTP_ID_RIBBONCUSTOMIZE_MORE
        MENUITEM "Näytä nauhan alla",                          XTP_ID_RIBBONCUSTOMIZE_QUICKACCESSBELOW_MORE
        MENUITEM "Näytä nauhan yläpuolella",                          XTP_ID_RIBBONCUSTOMIZE_QUICKACCESSABOVE_MORE
        MENUITEM SEPARATOR
        MENUITEM "Minimoi nauha",                            XTP_ID_RIBBONCUSTOMIZE_MINIMIZE
    END
END

