<Viewbox xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" Stretch="Uniform">
	<Canvas Width="32" Height="32">

		<Border Width="32" Height="32" Background="#E4E4E4"/>

		<Canvas Canvas.Left="4" Canvas.Top="4" Width="24" Height="24">
			<Rectangle Canvas.Left="0" Canvas.Top="0" Width="24" Height="24" Fill="#52419C"/>
			<Rectangle Canvas.Left="1" Canvas.Top="1" Width="22" Height="22" Fill="White"/>


			<Rectangle Canvas.Left="1" Canvas.Top="0" Width="22" Height="3">
				<Rectangle.Fill>
					<LinearGradientBrush StartPoint="0, 0" EndPoint="1,0">
						<GradientStop Color="#7887A2" Offset="0"/>
						<GradientStop Color="#3C4A65" Offset="1"/>
					</LinearGradientBrush>
				</Rectangle.Fill>
			</Rectangle>

			<Rectangle Canvas.Left="1" Canvas.Top="1" Width="22" Height="2">
				<Rectangle.Fill>
					<LinearGradientBrush StartPoint="0, 0" EndPoint="1,0">
						<GradientStop Color="#759CE5" Offset="0"/>
						<GradientStop Color="#4170CA" Offset="1"/>
					</LinearGradientBrush>
				</Rectangle.Fill>
			</Rectangle>

			<Polygon Points="2 4,  21 4,  21 18,  10,18  10 21,  2 21" Fill="#D7E4F3" Stroke="#52419C" StrokeDashArray="1"/>

			<Polygon Points=" 11.5 19,  21 19,  21 21,  11.5 21" Stroke="#52419C"/>
		</Canvas>
	</Canvas>
</Viewbox>
